<frame>
  <label :label="f'Count: {count}'" />
  <button label="bump" @clicked="bump" />
</frame>

<script>
import collagraph as cg


class Counter(cg.Component):
    def __init__(self, *args, **kwargs):
        super().__init__(*args, **kwargs)
        self.state["count"] = 0

    def bump(self):
        self.state["count"] += 1
</script>
